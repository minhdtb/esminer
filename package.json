{
  "name": "esminer",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "node build/dev-runner.js",
    "start:e": "electron ./dist",
    "dev:server": "node build/dev-server.js",
    "dev:client": "cross-env HOT=1 webpack --hide-modules --config build/webpack.dev-background.conf.js && cross-env HOT=1 electron",
    "build": "rimraf dist && mkdirp dist && cross-env NODE_ENV=production webpack --progress --hide-modules --config build/webpack.prod.conf.js",
    "package": "rimraf releases && node build/package.js --asar=true",
    "package:osx": "rimraf releases && node build/package.js --platform=darwin --arch=x64 --asar=true",
    "package:win": "rimraf releases && node build/package.js --platform=win32 --asar=true",
    "package:linux": "rimraf releases && node build/package.js --platform=linux --asar=true"
  },
  "author": "minhdtb",
  "license": "MIT",
  "dependencies": {
    "cross-env": "^5.0.5",
    "electron": "^1.7.5",
    "rimraf": "^2.6.1",
    "vue": "^2.4.2",
    "vue-router": "^2.7.0",
    "vuetify": "^0.14.10"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "electron-packager": "^8.7.2",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy-middleware": "^0.17.4",
    "minimist": "^1.2.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "tree-kill": "^1.1.0",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.5.9",
    "vue-loader": "^13.0.4",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.4",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0"
  }
}
